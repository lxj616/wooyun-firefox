     data-versions="/zh-CN/firefox/addon/4457/versions/"
     data-name="ProxySel"
     
       data-min="2.0"
     data-max="3.1b2"
     data-is-compatible="true"
     data-is-compatible-app="false"
     data-compat-overrides="[]"
