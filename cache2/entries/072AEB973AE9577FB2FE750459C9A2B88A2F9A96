    <a href="/zh-CN/firefox/addon/live-http-headers/?src=collection&amp;collection_id=6fa2752d-f181-3d1d-bccf-508f5ff7c939">
      <img class="icon" width="32" height="32" src="https://addons.cdn.mozilla.net/user-media/addon_icons/3/3829-32.png?modified=1352206517" alt="">
      Live HTTP Headers</a>
    <span>
        作者：<a href="/zh-CN/firefox/user/daniel-savard/" title="Daniel Savard">Daniel Savard</a>, <a href="/zh-CN/firefox/user/nikolasco/" title="Nikolas Co">Nikolas Co</a>
    </span>
  </h3>

    <div class="item-info">
      
<div class="install-shell">
<div class="install "
     data-addon="3829"
     data-icon="https://addons.cdn.mozilla.net/user-media/addon_icons/3/3829-32.png?modified=1352206517"
     data-developers="/zh-CN/firefox/addon/live-http-headers/developers"
     data-versions="/zh-CN/firefox/addon/3829/versions/"
     data-name="Live HTTP Headers"
     
       data-min="0.8"
     data-max="17.*"
     data-is-compatible="true"
     data-is-compatible-app="true"
     data-compat-overrides="[]"
     data-version="0.17.1-signed"
     data-compatible-apps="Firefox 0.8 及更新的版本"
